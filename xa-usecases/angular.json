{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xa-portal-dev": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/xa-portal-dev",
      "sourceRoot": "projects/xa-portal-dev/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/xa-portal-dev",
            "index": "projects/xa-portal-dev/src/index.html",
            "main": "projects/xa-portal-dev/src/main.ts",
            "polyfills": "projects/xa-portal-dev/src/polyfills.ts",
            "tsConfig": "projects/xa-portal-dev/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/xa-portal-dev/src/favicon.ico",
              "projects/xa-portal-dev/src/assets"
            ],
            "styles": [
              "projects/xa-portal-dev/src/styles.scss",
              "projects/xa-portal-dev/fonts/opensans.css",
              "projects/xa-portal-dev/fonts/fonts.css",
              "projects/xa-portal-dev/semantic/dist/semantic.css",
              "node_modules/overlayscrollbars/css/OverlayScrollbars.min.css",
              "node_modules/lightpick/css/lightpick.css",
              "node_modules/flatpickr/dist/flatpickr.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/overlayscrollbars/js/OverlayScrollbars.min.js",
              "projects/xa-portal-dev/semantic/dist/semantic.min.js",
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/xa-portal-dev/src/environments/environment.ts",
                  "with": "projects/xa-portal-dev/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "xa-portal-dev:build:production"
            },
            "development": {
              "browserTarget": "xa-portal-dev:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xa-portal-dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/xa-portal-dev/src/test.ts",
            "polyfills": "projects/xa-portal-dev/src/polyfills.ts",
            "tsConfig": "projects/xa-portal-dev/tsconfig.spec.json",
            "karmaConfig": "projects/xa-portal-dev/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/xa-portal-dev/src/favicon.ico",
              "projects/xa-portal-dev/src/assets"
            ],
            "styles": [
              "projects/xa-portal-dev/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
